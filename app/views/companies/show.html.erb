<h1><%=@company.name%></h1>
<h2><%=@company.city%>, <%=@company.state%></h2>

<h3>Contacts</h3>
<ul>
    <% for contact in @company.contacts%>
        <li>
            <%=contact.first_name%> <%contact.last_name%>
        </li>
    <%end%>

</ul>
<a href="/contacts/new?company_id=<%@company.id%>">Add new contact </a>

<a href="/companies">Back to all companies </a>